{
  "family": "mkdocs_family",
  "executionRoleArn": "arn:aws:iam::104300641251:role/ECS_Watch",
  "taskRoleArn": "arn:aws:iam::104300641251:role/ECS_Watch",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE", "EC2"],
  "cpu": "1024",
  "memory": "3072",
  "containerDefinitions": [
    {
      "name": "mkdocs-container",
      "image": "104300641251.dkr.ecr.ap-northeast-1.amazonaws.com/mkdocuments",
      "portMappings": [
        {
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ],
      "cpu": 1,
      "memory": 3072,
      "memoryReservation": 1024
    }
  ]
}
